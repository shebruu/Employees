{% extends 'base.html.twig' %}

{% block title %}Intern{% endblock %}

{% block body %}
    <h1>Intern</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ intern.id }}</td>
            </tr>
            <tr>
                <th>Fullname</th>
                <td>{{ intern.fullname }}</td>
            </tr>
            <tr>
                <th>StartDate</th>
                <td>{{ intern.startDate ? intern.startDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>EndDate</th>
                <td>{{ intern.endDate ? intern.endDate|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_intern_index') }}">back to list</a>

    <a href="{{ path('app_intern_edit', {'id': intern.id}) }}">edit</a>

    {{ include('intern/_delete_form.html.twig') }}
{% endblock %}
